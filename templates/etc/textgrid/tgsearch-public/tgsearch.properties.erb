# sesame config
sesame.endpoint = http://localhost:9091/openrdf-sesame/repositories/textgrid-public

# elastic search config
elasticSearch.url = 127.0.0.1
elasticSearch.port = <%= scope.lookupvar('dhrep::services::intern::tgelasticsearch::master_tcp_port') %>
elasticSearch.clusterName = <%= scope.lookupvar('dhrep::services::intern::tgelasticsearch::cluster_name') %>
elasticSearch.index = textgrid-public
elasticSearch.type = metadata
elasticSearch.itemLimit = 3000
