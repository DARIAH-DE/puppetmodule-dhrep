<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Publikator standalone</display-name>
  <env-entry>
    <description>Enable AAI</description>
    <env-entry-name>enableAAI</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::enable_aai') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Enable subcollections</description>
    <env-entry-name>subcollectionsEnabled</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::subcollections_enabled') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>DARIAH-DE OwnStorage URL</description>
    <env-entry-name>dariahStorageUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::dariah_storage_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>DH-publish URL</description>
    <env-entry-name>dhpublishUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::dhpublish_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>DH-crud URL</description>
    <env-entry-name>dhcrudUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::dhcrud_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Collection Registry URL</description>
    <env-entry-name>collectionRegistryUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::collection_registry_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Generic Search URL</description>
    <env-entry-name>genericSearchUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::generic_search_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>PID Service URL</description>
    <env-entry-name>pidServiceUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::pid_service_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Offset to refresh status info in publishing process</description>
    <env-entry-name>refresh</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::refresh') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Default timeout for service calls</description>
    <env-entry-name>serviceTimeout</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::service_timeout') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Enable Seafile collections</description>
    <env-entry-name>seafileEnabled</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::seafile_enabled') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Seafile URL</description>
    <env-entry-name>seafileUrl</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::seafile_url') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Secret for decrypting Seafile token</description>
    <env-entry-name>seafileTokenSecret</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::seafile_token_secret') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>SSLCertVerification</description>
    <env-entry-name>SSLCertVerification</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::ssl_cert_verification') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Skip publish status check (faster, for GUI debugging)</description>
    <env-entry-name>skipPublishStatusCheck</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::skip_publish_status_check') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Use dryrun instead of real publishing</description>
    <env-entry-name>dryrun</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::dryrun') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Debug mode</description>
    <env-entry-name>debug</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::debug') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Override user's ePPN with that one from below</description>
    <env-entry-name>overrideEppn</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::override_eppn') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Always publish with this ePPN, overrides user's ePPN</description>
    <env-entry-name>eppn</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::eppn') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Link to Documentation</description>
    <env-entry-name>linkToDocumentation</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::link_to_documentation') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Link to Bug Tracker</description>
    <env-entry-name>linkToBugtracker</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::link_to_bugtracker') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Name of Contact</description>
    <env-entry-name>nameOfContact</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::name_of_contact') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Mail of Contact</description>
    <env-entry-name>mailOfContact</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::mail_of_contact') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Redis Server host</description>
    <env-entry-name>redisHostname</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::redis_hostname') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Redis Server Port</description>
    <env-entry-name>redisPort</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::redis_port') %></env-entry-value>
  </env-entry>
  <env-entry>
    <description>Redis maximum number of connections</description>
    <env-entry-name>redisMaxParallel</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value><%= scope.lookupvar('dhrep::services::publikator::redis_max_parallel') %></env-entry-value>
  </env-entry>
  <welcome-file-list>
    <welcome-file>mainView</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
