AddDefaultCharset         UTF-8
SetEnv PYTHONIOENCODING   utf8
Options FollowSymLinks    ExecCGI
RewriteEngine             On

RewriteRule	^api/p$                                     /marketplace/cgi/msInterface.cgi?action=main
RewriteRule	^catalogs/api/p$                            /marketplace/cgi/msInterface.cgi?action=catalogs
RewriteRule	^taxonomy/term/(.*?),(.*?)/api/p            /marketplace/cgi/msInterface.cgi?action=taxonomy&marketId=$1&categoryId=$2
RewriteRule	^featured/api/p$                            /marketplace/cgi/msInterface.cgi?action=list&type=featured
RewriteRule	^featured/(.*?)/api/p$                      /marketplace/cgi/msInterface.cgi?action=list&type=featured&marketId=$1
RewriteRule	^recent/api/p$                              /marketplace/cgi/msInterface.cgi?action=list&type=recent
RewriteRule	^favorites/top/api/p$                       /marketplace/cgi/msInterface.cgi?action=list&type=favorites
RewriteRule	^popular/top/api/p$                         /marketplace/cgi/msInterface.cgi?action=list&type=popular
RewriteRule	^node/(.*?)/api/p$                          /marketplace/cgi/msInterface.cgi?action=content&plugId=$1
#RewriteRule	^api/p/search/apachesolr_search/(.*?)\?filters=(.*?)$	http://<%= scope.lookupvar('::fqdn') %>/marketplace/cgi/msInterface.cgi?action=search&query=$1&filter=$2
RewriteRule	^api/p/search/apachesolr_search/(.*?)$      /marketplace/cgi/msInterface.cgi?action=search&query=$1
RewriteRule	^content/(.*?)/api/p$                       /marketplace/cgi/msInterface.cgi?action=content&plugId=$1
RewriteRule	^content/(.*?)$                             /marketplace/cgi/msInterface.cgi?action=redirect&plugId=$1

ReWriteRule	^$                                          /marketplace/cgi/msInterface.cgi?action=goto_wiki
