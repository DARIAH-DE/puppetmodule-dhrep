<%
replication = scope.lookupvar('dhrep::services::tgauth::ldap_replication')
clusternodes = scope.lookupvar('dhrep::services::tgauth::ldap_clusternodes')
%>

# extended LDIF
#
# LDAPv3
# base <cn=config> with scope subtree
# filter: (objectclass=*)
# requesting: ALL
#

# config
dn: cn=config
objectClass: olcGlobal
cn: config
olcConfigDir: /etc/ldap/slapd.d
olcArgsFile: /var/run/slapd/slapd.args
olcAttributeOptions: lang-
olcAuthzPolicy: none
olcConcurrency: 32
olcConnMaxPending: 100
olcConnMaxPendingAuth: 1000
olcGentleHUP: FALSE
olcIdleTimeout: 0
olcIndexSubstrIfMaxLen: 4
olcIndexSubstrIfMinLen: 2
olcIndexSubstrAnyLen: 4
olcIndexSubstrAnyStep: 2
olcIndexIntLen: 4
olcLocalSSF: 71
olcLogLevel: Stats
olcPasswordCryptSaltFormat: $1$%.8s
olcPidFile: /var/run/slapd/slapd.pid
olcReadOnly: FALSE
olcReverseLookup: FALSE
olcSaslSecProps: noplain,noanonymous
olcSockbufMaxIncoming: 262143
olcSockbufMaxIncomingAuth: 16777215
olcThreads: 32
#olcTLSCACertificateFile: /etc/shibboleth/daasi-ca.pem
#olcTLSCertificateFile: /etc/shibboleth/textgrid-ws3.pem
#olcTLSCertificateKeyFile: /etc/shibboleth/textgrid-ws3.key
olcTLSVerifyClient: never
olcToolThreads: 1
olcWriteTimeout: 0
<% if replication -%>
# for multimaster replication
<% clusternodes.to_enum.with_index(1).each do |node,idx| -%>
olcServerID: <%= idx %> ldap://<%= node %>
<% end -%>
<% end -%>

# module{0}, config
dn: cn=module{0},cn=config
objectClass: olcModuleList
cn: module{0}
olcModulePath: /usr/lib/ldap
olcModuleLoad: {0}back_mdb
<% if replication -%>
# accesslog & syncprov for multi-master replication
olcModuleLoad: {1}accesslog
olcModuleLoad: {2}syncprov
<% end -%>

